<%= search_form_for @q do |f| %>
    <div class="row">
      <div class="col-md-9 col-lg-9">
        <%= f.search_field :pacient_first_name_or_pacient_last_name_cont, class: 'form-control', placeholder: 'Buscar...' %>
      </div>
      <div class="col-md-3 col-lg-3 pull-right">
        <button type="submit" class="btn btn-default" aria-label="Left Align">
          <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
        </button>
        <%= link_to medical_consultation_payments_path, class: 'btn btn-danger' do %>
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        <% end %>
      </div>
    </div>
<% end %>